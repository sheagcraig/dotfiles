[user]
	name = Shea Craig
	email = shea.craig@da.org
[core]
	editor = /usr/bin/vim
	excludesfile = ~/.gitignore_global
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[push]
	default = simple
[credential]
	helper = osxkeychain
